<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="github" value="https://nuget.pkg.github.com/%GITHUB_PACKAGES_OWNER%/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <github>
        <add key="Username" value="%GITHUB_PACKAGES_USERNAME%" />
        <add key="ClearTextPassword" value="%GITHUB_PACKAGES_ACCESS_TOKEN%" />
      </github>
  </packageSourceCredentials>
</configuration>
